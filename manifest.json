{
  "manifest_version": 3,
  "name": "Monitor de Requisições",
  "version": "1.0",
  "description": "Monitora requisições para URLs específicas e as envia para um backend.",
  "permissions": [
    "webRequest",
    "declarativeNetRequest",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}